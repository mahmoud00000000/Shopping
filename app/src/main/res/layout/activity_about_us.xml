<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.AboutUs">
    
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="414dp"
        android:layout_height="115dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="#D6D6D6">
        
        <TextView
            android:layout_width="52dp"
            android:layout_height="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="58dp"
            android:layout_marginBottom="27dp"
            android:layout_marginStart="181dp"
            android:layout_marginEnd="181dp"
            android:text="من نحن"
            android:textColor="@color/black"
            android:textSize="16sp"/>
        
        <ImageView
            android:id="@+id/aboutus_back_btn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="63dp"
            android:layout_marginBottom="32dp"
            android:layout_marginStart="379dp"
            android:layout_marginEnd="15dp"
            android:src="@drawable/baseline_arrow_black_back_24"/>
        
    </androidx.constraintlayout.widget.ConstraintLayout>
    
    <TextView
        android:layout_width="384dp"
        android:layout_height="400dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="80dp"
        android:textSize="18sp"
        android:textColor="#1F115F"
        android:paddingTop="80dp"
        android:text="shopping هي شركة متخصصة في إنتاج وبيع الهوديز عالية الجودة التي تجمع بين الراحة والأناقة والتنوع. تقدم الشركة مجموعة واسعة من الهوديز بتصاميم وألوان وأحجام مختلفة لتناسب جميع الأذواق والمناسبات. سواء كنت تبحث عن ملابس مريحة للاستخدام اليومي أو أزياء عصرية تعكس شخصيتك، فإن منتجاتنا تتميز بأقمشة فاخرة وتصاميم عصرية تجمع بين الموضة والوظيفية. في Shopping، نلتزم بتقديم منتجات عالية الجودة تلبي احتياجات عملائنا وتجعلنا الوجهة المثالية لعشاق الهوديز."/>



</androidx.constraintlayout.widget.ConstraintLayout>