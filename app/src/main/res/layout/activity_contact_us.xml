<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home.ContactUs">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="414dp"
        android:layout_height="115dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="#D6D6D6">

        <TextView
            android:layout_width="80dp"
            android:layout_height="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="58dp"
            android:layout_marginBottom="27dp"
            android:layout_marginStart="181dp"
            android:layout_marginEnd="181dp"
            android:text="تواصل معنا"
            android:textColor="@color/black"
            android:textSize="16sp"/>

        <ImageView
            android:id="@+id/contact_back_btn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="63dp"
            android:layout_marginBottom="32dp"
            android:layout_marginStart="379dp"
            android:layout_marginEnd="15dp"
            android:src="@drawable/baseline_arrow_black_back_24"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:layout_width="91dp"
        android:layout_height="29dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="130dp"
        android:layout_marginBottom="550dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="308dp"
        android:text="اسمك بالكامل"
        android:textSize="15sp"
        android:textColor="@color/black"/>

    <EditText
        android:layout_width="384dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="169dp"
        android:layout_marginBottom="520dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:hint="ادخل اسمك بالكامل"
        android:textColorHint="#B1B0B0"
        android:inputType="text"
        android:textColor="@color/black"/>

    <TextView
        android:layout_width="105dp"
        android:layout_height="29dp"
        android:textSize="15sp"
        android:text="البريد الالكتروني"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="234dp"
        android:layout_marginBottom="480dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="294dp"/>

    <EditText
        android:layout_width="384dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="273dp"
        android:layout_marginBottom="450dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:hint="ادخل البريد الالكتروني"
        android:textColorHint="#B1B0B0"
        android:inputType="textEmailAddress"
        android:textColor="@color/black"/>

    <TextView
        android:layout_width="66dp"
        android:layout_height="29dp"
        android:text="استفسارك"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="338dp"
        android:layout_marginBottom="393dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="333dp"/>

    <EditText
        android:layout_width="384dp"
        android:layout_height="162dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="380dp"
        android:layout_marginBottom="250dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:hint="استفسارك"
        android:textColorHint="#B1B0B0"
        android:textColor="@color/black"
        android:paddingBottom="110dp"/>

    <ImageView
        android:layout_width="384dp"
        android:layout_height="50dp"
        android:background="#D6D6D6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="500dp"
        android:layout_marginBottom="100dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"/>

    <TextView
        android:layout_width="38dp"
        android:layout_height="29dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="520dp"
        android:layout_marginBottom="115dp"
        android:layout_marginStart="188dp"
        android:layout_marginEnd="188dp"
        android:text="ارسال"
        android:textColor="@color/black"
        android:textSize="15sp"/>

</androidx.constraintlayout.widget.ConstraintLayout>